<%- include('../partials/header')%>

    <h1>MATCHES!!!!</h1>


    <% if (matches.length > 0) { %>
        <% matches.forEach(function(match){ %>

            <div>
                <%= match.name %>
                <%= match.location %>
            </div>

        <% }) %>
    <% } else { %>
            <p>No matches yet!</p>
        <% } %>

